{
	"name": "ReddThat",
	"version": "1.1.0",
	"description": "A Reddit client for the desktop",
	"author": "Micah Lindley",
	"maintainer": "Micah Lindley",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "./node_modules/.bin/electron-builder --linux snap"
	},
	"build": {
		"appId": "reddThat",
		"linux": {
			"category": "Utility",
			"maintainer": "Micah Lindley <hi@micahlindley.com>"
		}
	},
	"postinstall": "electron-builder install-app-deps",
	"repository": "https://github.com/micahlt/reddthat",
	"license": "MIT",
	"devDependencies": {
		"electron": "^8.2.0",
		"electron-builder": "^22.4.1"
	},
	"dependencies": {}
}
